{
  "name": "ytestbook",
  "version": "0.1.0",
  "private": true,
  "source": "public/index.html",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./.parcel-cache",
    "install": "yarn install:api",
    "install:api": "openapi-generator-cli generate -i api/ytestbook.yml -g typescript-rxjs -c openapi-generator.config.json -o src/generated/",
    "start": "yarn run dev",
    "dev": "yarn run clean && npx msw init dist/ --save && parcel -p 3000",
    "build": "yarn run clean && parcel build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.16",
    "@mantine/dates": "^6.0.16",
    "@mantine/form": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@mantine/modals": "^6.0.16",
    "@mantine/notifications": "^6.0.16",
    "@mantine/tiptap": "^6.0.16",
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@parcel/transformer-svg-react": "^2.7.0",
    "@tabler/icons-react": "^2.25.0",
    "@types/lodash": "^4.14.195",
    "@xstate/react": "^3.2.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "immer": "^10.0.2",
    "lodash": "^4.17.21",
    "mantine-react-table": "^1.0.0-beta.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router": "6.14.1",
    "react-router-dom": "6.14.1",
    "xstate": "^4.38.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.3",
    "msw": "^1.2.2",
    "parcel": "^2.9.3",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "msw": {
    "workerDirectory": "dist"
  }
}