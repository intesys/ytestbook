{
  "name": "ytestbook",
  "version": "0.1.0",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./.parcel-cache",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "dev": "yarn run clean && parcel -p 3000",
    "build": "yarn run clean && parcel build",
    "export": "yarn run clean && parcel build --no-source-maps --dist-dir dist/admin",
    "server": "pouchdb-server --host 0.0.0.0 -p 10102 -m -d /tmp/pouchdb-server/ -n true",
    "serve": "serve ./dist",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@mantine/core": "^4.0.0",
    "@mantine/dates": "^4.0.0",
    "@mantine/hooks": "^4.0.0",
    "@mantine/modals": "^4.0.0",
    "@mantine/notifications": "^4.0.0",
    "@mantine/prism": "^4.0.0",
    "@mantine/rte": "^4.0.0",
    "@radix-ui/react-icons": "^1.1.0",
    "concurrently": "6.5.1",
    "dayjs": "^1.10.8",
    "immer": "^9.0.12",
    "pouchdb-adapter-http": "7.2.2",
    "pouchdb-adapter-idb": "7.2.2",
    "pouchdb-replication": "7.2.2",
    "pouchdb-server": "4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "rxdb": "^11.6.0",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.24",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.3",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "jest": "^27.5.1",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "typescript": "^4.5.5"
  }
}
