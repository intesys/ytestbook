{
  "name": "ytestbook",
  "version": "0.1.0",
  "private": true,
  "source": "src/index.html",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./.parcel-cache",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "dev": "yarn run clean && parcel -p 3000",
    "build": "yarn run clean && parcel build",
    "export": "yarn run clean && parcel build --no-source-maps --dist-dir dist/admin",
    "server": "pouchdb-server --host 0.0.0.0 -p 10102 -m -d /tmp/pouchdb-server/ -n true",
    "serve": "serve ./dist",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@mantine/core": "^6.0.16",
    "@mantine/dates": "^6.0.16",
    "@mantine/form": "^6.0.16",
    "@mantine/hooks": "^6.0.16",
    "@mantine/modals": "^6.0.16",
    "@mantine/notifications": "^6.0.16",
    "@types/lodash": "^4.17.21",
    "concurrently": "^8.2.0",
    "immer": "^10.0.2",
    "lodash": "^4.17.21",
    "pouchdb-adapter-http": "7.2.2",
    "pouchdb-adapter-idb": "7.2.2",
    "pouchdb-replication": "7.2.2",
    "pouchdb-server": "4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-router": "6.14.1",
    "react-router-dom": "6.14.1",
    "rxdb": "^14.14.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.3",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "jest": "^27.5.1",
    "parcel": "^2.9.3",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "typescript": "^5.1.6"
  }
}
